import random
secret_number = random.randint(1, 10)

while True:
    number=int(input("enter a number"))
    #Create a variable to store user guesses
    #You fill this out
   
    #Use an if-elif-else chain to let the user know if their guess was right, too high, or too low
    #You fill this out
    if number > secret_number:
        print ("too high")
    elif number < secret_number:
        print ("too low")
    else:
        print (" you got it yay")
